{
  "name": "@mixor/core",
  "version": "1.0.0",
  "description": "Mixor core library with abstractions, design patterns and utilities",
  "author": "Carlos Cosming",
  "license": "MIT",
  "keywords": [
    "design-patterns",
    "utilities"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "test": "moonx test -s -u",
    "test:types": "moonx test-types -s -u",
    "test:runtime": "moonx test-runtime -s -u",
    "test:watch": "moonx test-watch -s -u",
    "test:project": "pnpm vitest --typecheck --hideSkippedTests --bail 1 --coverage --project",
    "build": "moonx build -s -u"
  },
  "devDependencies": {
    "@unokit/tsup": "^1.0.0",
    "@unokit/vitest": "^1.0.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "vitest": "^3.1.4"
  }
}
